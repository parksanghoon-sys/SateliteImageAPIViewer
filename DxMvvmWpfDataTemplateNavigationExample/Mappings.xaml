<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dataContext ="clr-namespace:DxMvvmWpfDataTemplateNavigationExample.ViewModels"
                    xmlns:views="clr-namespace:DxMvvmWpfDataTemplateNavigationExample.Views">
    
    <DataTemplate DataType="{x:Type dataContext:MainWindowViewModel}">
        <views:MainWindow/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:SubView1ViewModel}">
        <views:SubView1/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dataContext:SubView2ViewModel}">
        <views:SubView2/>
    </DataTemplate>


</ResourceDictionary>